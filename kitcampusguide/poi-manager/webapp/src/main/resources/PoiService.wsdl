						</xsd:element>
						<xsd:element name="successMessage" type="xsd:string">
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="CreateFault">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="faultMessage" type="xsd:string">
						</xsd:element>
						<xsd:element name="poi" type="tns:Poi">
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Delete">
				<xsd:complexType>
					<xsd:sequence maxOccurs="1" minOccurs="1">
						<xsd:element name="id" type="xsd:int"></xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="DeleteResponse">
				<xsd:complexType>
					<xsd:sequence maxOccurs="1" minOccurs="1">
						<xsd:element name="successMessage" type="xsd:string"></xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="DeleteFault">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="faultMessage" type="xsd:string">
						</xsd:element>
						<xsd:element name="id" type="xsd:int">
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Update">
				<xsd:complexType>
					<xsd:sequence maxOccurs="1" minOccurs="1">
						<xsd:element name="poi" type="tns:PoiWithId"></xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="UpdateResponse">
				<xsd:complexType>
					<xsd:sequence maxOccurs="1" minOccurs="1">
						<xsd:element name="poi" type="tns:PoiWithId"></xsd:element>
						<xsd:element name="successMessage" type="xsd:string">
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="UpdateFault">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="faultMessage" type="xsd:string">
						</xsd:element>
						<xsd:element name="poi" type="tns:PoiWithId">
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Read">
				<xsd:complexType>
					<xsd:sequence maxOccurs="1" minOccurs="1">
						<xsd:element name="id" type="xsd:int"></xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="ReadResponse">
				<xsd:complexType>
					<xsd:sequence maxOccurs="1" minOccurs="1">
						<xsd:element name="poi" type="tns:PoiWithId"></xsd:element>
						<xsd:element name="successMessage" type="xsd:string">
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="ReadFault">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="id" type="xsd:int">
						</xsd:element>
						<xsd:element name="faultMessage" type="xsd:string">
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Select">
				<xsd:complexType>
					<xsd:choice maxOccurs="1" minOccurs="0">
						<xsd:element name="findByNamesLike" type="tns:Names">
						</xsd:element>
						<xsd:element name="findByNames" type="tns:Names">
						</xsd:element>
						<xsd:element name="findByNameSuffixes" type="tns:Names">
						</xsd:element>
						<xsd:element name="findByNamePrefixes" type="tns:Names">
						</xsd:element>
						<xsd:element name="findByIds" type="tns:Ids">
						</xsd:element>
					</xsd:choice>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="SelectResponse">
				<xsd:complexType>
					<xsd:sequence maxOccurs="1" minOccurs="1">
						<xsd:element name="successMessage" type="xsd:string">
						</xsd:element>
						<xsd:sequence maxOccurs="unbounded" minOccurs="0">
							<xsd:element name="poi" type="tns:PoiWithId"></xsd:element>
						</xsd:sequence>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="SelectFault">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="faultMessage" type="xsd:string">
						</xsd:element>
						<xsd:choice>
							<xsd:element name="findByNamesLike" type="tns:Names">
							</xsd:element>
							<xsd:element name="findByNames" type="tns:Names">
							</xsd:element>
							<xsd:element name="findByNameSuffixes" type="tns:Names">
							</xsd:element>
							<xsd:element name="findByNamePrefixes" type="tns:Names">
							</xsd:element>
							<xsd:element name="findByIds" type="tns:Ids">
							</xsd:element>
						</xsd:choice>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>

			<xsd:complexType name="Names">
				<xsd:sequence>
					<xsd:element name="name" type="xsd:string" maxOccurs="unbounded"
						minOccurs="1"></xsd:element>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:complexType name="Ids">
				<xsd:sequence>
					<xsd:element name="id" type="xsd:int" maxOccurs="unbounded"
						minOccurs="1"></xsd:element>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:complexType name="Poi">
				<xsd:sequence maxOccurs="1" minOccurs="1">
					<xsd:element name="name" type="xsd:string"></xsd:element>
					<xsd:element name="categoryName" type="xsd:string"></xsd:element>
					<xsd:element name="description" type="xsd:string"></xsd:element>
					<xsd:element name="longitude" type="xsd:double"></xsd:element>
					<xsd:element name="latitude" type="xsd:double"></xsd:element>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:complexType name="PoiWithId">
				<xsd:sequence maxOccurs="1" minOccurs="1">
					<xsd:element name="id" type="xsd:int"></xsd:element>
					<xsd:element name="name" type="xsd:string"></xsd:element>
					<xsd:element name="categoryName" type="xsd:string"></xsd:element>
					<xsd:element name="description" type="xsd:string"></xsd:element>
					<xsd:element name="longitude" type="xsd:double"></xsd:element>
					<xsd:element name="latitude" type="xsd:double"></xsd:element>
				</xsd:sequence>
			</xsd:complexType>
		</xsd:schema>
