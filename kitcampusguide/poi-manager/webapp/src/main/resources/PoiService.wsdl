	</wsdl:message>
	<wsdl:message name="Delete">
		<wsdl:part name="parameters" element="tns:Delete"></wsdl:part>
	</wsdl:message>
	<wsdl:message name="DeleteResponse">
		<wsdl:part name="parameters" element="tns:DeleteResponse"></wsdl:part>
	</wsdl:message>
	<wsdl:message name="DeleteFault">
		<wsdl:part name="parameters" element="tns:DeleteFault"></wsdl:part>
	</wsdl:message>
	<wsdl:message name="Update">
		<wsdl:part name="parameters" element="tns:Update"></wsdl:part>
	</wsdl:message>
	<wsdl:message name="UpdateResponse">
		<wsdl:part name="parameters" element="tns:UpdateResponse"></wsdl:part>
	</wsdl:message>
	<wsdl:message name="UpdateFault">
		<wsdl:part name="parameters" element="tns:UpdateFault"></wsdl:part>
	</wsdl:message>
	<wsdl:message name="Read">
		<wsdl:part name="parameters" element="tns:Read"></wsdl:part>
	</wsdl:message>
	<wsdl:message name="ReadResponse">
		<wsdl:part name="parameters" element="tns:ReadResponse"></wsdl:part>
	</wsdl:message>
	<wsdl:message name="ReadFault">
		<wsdl:part name="parameters" element="tns:ReadFault"></wsdl:part>
	</wsdl:message>
	<wsdl:message name="Select">
		<wsdl:part name="parameters" element="tns:Select"></wsdl:part>
	</wsdl:message>
	<wsdl:message name="SelectResponse">
		<wsdl:part name="parameters" element="tns:SelectResponse"></wsdl:part>
	</wsdl:message>
	<wsdl:message name="SelectFault">
		<wsdl:part name="parameters" element="tns:SelectFault"></wsdl:part>
	</wsdl:message>
	<wsdl:portType name="PoiService">
		<wsdl:operation name="Create">
			<wsdl:input message="tns:Create"></wsdl:input>
			<wsdl:output message="tns:CreateResponse"></wsdl:output>
			<wsdl:fault name="fault" message="tns:CreateFault"></wsdl:fault>
